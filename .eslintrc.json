{
  "root": true,
  "extends": ["nth"],
  "env": {
    "node": true,
    "jest": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2020
  },
  "rules": {
    "comma-dangle": "off",
    "space-before-function-paren": "off",
    "object-curly-spacing": "off",
    "no-magic-numbers": "off",
    "no-multiple-empty-lines": "off",
    "max-len": "off"
  }
}
